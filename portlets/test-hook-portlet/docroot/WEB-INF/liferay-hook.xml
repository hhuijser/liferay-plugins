<?xml version="1.0"?>
<!DOCTYPE hook PUBLIC "-//Liferay//DTD Hook 6.2.0//EN" "http://www.liferay.com/dtd/liferay-hook_6_2_0.dtd">

<hook>
	<portal-properties>portal.properties</portal-properties>
	<language-properties>content/Language*.properties</language-properties>
	<custom-jsp-dir>/META-INF/custom_jsps</custom-jsp-dir>
	<service>
		<service-type>com.liferay.portal.service.UserLocalService</service-type>
		<service-impl>com.liferay.testhook.hook.service.impl.TestHookUserLocalServiceImpl</service-impl>
	</service>
	<servlet-filter>
		<servlet-filter-name>Able Test Hook Filter</servlet-filter-name>
		<servlet-filter-impl>com.liferay.testhook.hook.filter.TestHookFilter</servlet-filter-impl>
	</servlet-filter>
	<servlet-filter>
		<servlet-filter-name>Baker Test Hook Filter</servlet-filter-name>
		<servlet-filter-impl>com.liferay.testhook.hook.filter.TestHookFilter</servlet-filter-impl>
	</servlet-filter>
	<servlet-filter>
		<servlet-filter-name>Charlie Test Hook Filter</servlet-filter-name>
		<servlet-filter-impl>com.liferay.testhook.hook.filter.TestHookFilter</servlet-filter-impl>
	</servlet-filter>
	<servlet-filter-mapping>
		<servlet-filter-name>Able Test Hook Filter</servlet-filter-name>
		<before-filter>Baker Test Hook Filter</before-filter>
		<url-pattern>/*</url-pattern>
	</servlet-filter-mapping>
	<servlet-filter-mapping>
		<servlet-filter-name>Baker Test Hook Filter</servlet-filter-name>
		<url-pattern>/*</url-pattern>
	</servlet-filter-mapping>
	<servlet-filter-mapping>
		<servlet-filter-name>Charlie Test Hook Filter</servlet-filter-name>
		<after-filter>Baker Test Hook Filter</after-filter>
		<url-pattern>/*</url-pattern>
	</servlet-filter-mapping>
</hook>